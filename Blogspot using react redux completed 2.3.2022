export const fetchUsers = () => {
  return (dispatch) => {
    axios.get(`http://localhost:8080/api/users/blogs`).then((response) => {
      const data = response.data;
      dispatch(fetchUsersSuccess(data));
    });
  };
};

export const fetchUsersSuccess = (data) => {
  return {
    type: FETCH_USERS_SUCCESS,
    payload: data,
  };
};


