-------------------updown Float motion farmer--------------------


const loaderVariants = {
 
  animationTwo: {
    y: [0, -40],
    x: 0,
    transition: {
      y: {
        yoyo: Infinity,
        duration: 0.25,
        ease: 'easeOut'
      }
    }
  }
};


  <motion.div className="loader"
     variants={loaderVariants}
     animate={"animationTwo"}
     
   >
  

</motion.div>

